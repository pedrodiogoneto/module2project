<h2 class="title">My Venues</h2>

<a href="/venues/new-venue"><button id = "create-venue-button" class="btn btn-dark">Create new</button></a>

<ul>
    <% if (Object.keys(venues).length !== 0) { %>
        <% venues.forEach( (venue) => { %>
            <div class="main-list">
            <li class="venue-details venues-list">
                <a class="name" href="/venues/<%= venue._id %>"><%= venue.name %></a>        
            </li>
        </div>
        <% }); %>
    <% } else { %>
        <p>You haven't yet created a venue</p>
    <% }%>
</ul>

